{
  "name": "got-balls-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "ts-node --project ./tsconfig.node.json ./scripts/gen-antd-css.tsx",
    "dev": "pnpm predev && next dev",
    "prebuild": "cross-env NODE_ENV=production ts-node --project ./tsconfig.node.json ./scripts/gen-antd-css.tsx",
    "build": "pnpm prebuild && next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "@types/node": "18.16.3",
    "@types/react": "18.2.5",
    "@types/react-dom": "18.2.3",
    "accept-language": "^3.0.18",
    "antd": "^5.4.6",
    "autoprefixer": "10.4.14",
    "eslint": "8.39.0",
    "eslint-config-next": "13.4.0",
    "next": "13.4.0",
    "next-auth": "^4.22.1",
    "next-intl": "^2.13.2",
    "postcss": "8.4.23",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "@ant-design/static-style-extract": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "cross-env": "^7.0.3",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint \"src/**/*.ts\""
  }
}
